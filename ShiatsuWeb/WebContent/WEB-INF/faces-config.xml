<?xml version="1.0" encoding="UTF-8"?>

<faces-config
    xmlns="http://java.sun.com/xml/ns/javaee"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-facesconfig_2_0.xsd"
    version="2.0">
	<application>
		<el-resolver>org.springframework.web.jsf.el.SpringBeanFacesELResolver</el-resolver>
		<message-bundle>com.shiatsu.web.bundles.recursos</message-bundle>
	</application>
	 <!-- ========================   Managed Beans =============================== -->
	 
	 
	<!-- ================ Managed Bean ClienteController ================ -->
	 <managed-bean>
	        <managed-bean-name>clienteController</managed-bean-name>
	        <managed-bean-class>com.shiatsu.web.controller.ClienteController</managed-bean-class>
	        <managed-bean-scope>session</managed-bean-scope>
	        <managed-property>
	            <property-name>clienteBo</property-name>
	            <property-class>com.shiatsu.bo.ClienteBo</property-class>
	            <value>#{clienteBo}</value>
        	</managed-property>
	 </managed-bean>
	 
	 <!-- ================ Managed Bean TipoAlopeciaController ================ -->
	 <managed-bean>
	        <managed-bean-name>tipoAlopeciaController</managed-bean-name>
	        <managed-bean-class>com.shiatsu.web.controller.TipoAlopeciaController</managed-bean-class>
	        <managed-bean-scope>session</managed-bean-scope>
	        <managed-property>
	            <property-name>tipoAlopeciaBo</property-name>
	            <property-class>com.shiatsu.bo.TipoAlopeciaBo</property-class>
	            <value>#{tipoAlopeciaBo}</value>
        	</managed-property>
	 </managed-bean>
	 
	 <!-- ================ Managed Bean PaisController ================ -->
	 <managed-bean>
	        <managed-bean-name>paisController</managed-bean-name>
	        <managed-bean-class>com.shiatsu.web.controller.PaisController</managed-bean-class>
	        <managed-bean-scope>session</managed-bean-scope>
	        <managed-property>
	            <property-name>paisBo</property-name>
	            <property-class>com.shiatsu.bo.PaisBo</property-class>
	            <value>#{paisBo}</value>
        	</managed-property>
	 </managed-bean>
	 
	 <!-- ================ Managed Bean ProfesionController ================ -->
	 <managed-bean>
	        <managed-bean-name>profesionController</managed-bean-name>
	        <managed-bean-class>com.shiatsu.web.controller.ProfesionController</managed-bean-class>
	        <managed-bean-scope>session</managed-bean-scope>
	        <managed-property>
	            <property-name>profesionBo</property-name>
	            <property-class>com.shiatsu.bo.ProfesionBo</property-class>
	            <value>#{profesionBo}</value>
        	</managed-property>
	 </managed-bean>
	 <!-- *************************  NavegaciÃ³n ************************************* -->

	<!-- ================ Navigation ClienteController ================ -->
	 <navigation-rule>
	    	<from-view-id>/shiatsuListaClientes.jsp</from-view-id>
	    	<navigation-case>
	    		<from-action>#{clienteController.cargarObjeto}</from-action>
	    		<from-outcome>success</from-outcome>
	    		<to-view-id>/shiatsuActCliente.jsp</to-view-id>
	    	</navigation-case>
	    	<navigation-case>
	    		<from-action>#{clienteController.irAgregar}</from-action>
	    		<from-outcome>success</from-outcome>
	    		<to-view-id>/shiatsuActCliente.jsp</to-view-id>
	    	</navigation-case>
	    </navigation-rule>
		<navigation-rule>
	    	<from-view-id>/shiatsuActCliente.jsp</from-view-id>
	    	<navigation-case>
	    		<from-action>#{clienteController.regresar}</from-action>
	    		<from-outcome>success</from-outcome>
	    		<to-view-id>/shiatsuListaClientes.jsp</to-view-id>
	    	</navigation-case>
	    	<navigation-case>
	    		<from-action>#{clienteController.eliminar}</from-action>
	    		<from-outcome>success</from-outcome>
	    		<to-view-id>/shiatsuListaClientes.jsp</to-view-id>
	    	</navigation-case>
	    </navigation-rule>
	    
	<!-- ================ Navigation PaisController ================ -->
	 <navigation-rule>
	    	<from-view-id>/shiatsuListaPais.jsp</from-view-id>
	    	<navigation-case>
	    		<from-action>#{paisController.cargarObjeto}</from-action>
	    		<from-outcome>success</from-outcome>
	    		<to-view-id>/shiatsuActPais.jsp</to-view-id>
	    	</navigation-case>
	    	<navigation-case>
	    		<from-action>#{paisController.irAgregar}</from-action>
	    		<from-outcome>success</from-outcome>
	    		<to-view-id>/shiatsuActPais.jsp</to-view-id>
	    	</navigation-case>
	    </navigation-rule>
		<navigation-rule>
	    	<from-view-id>/shiatsuActPais.jsp</from-view-id>
	    	<navigation-case>
	    		<from-action>#{paisController.regresar}</from-action>
	    		<from-outcome>success</from-outcome>
	    		<to-view-id>/shiatsuListaPais.jsp</to-view-id>
	    	</navigation-case>
	    	<navigation-case>
	    		<from-action>#{paisController.eliminar}</from-action>
	    		<from-outcome>success</from-outcome>
	    		<to-view-id>/shiatsuListaPais.jsp</to-view-id>
	    	</navigation-case>
	    </navigation-rule>
		
		
			<!-- ================ Navigation ProfesionController ================ -->
	 <navigation-rule>
	    	<from-view-id>/shiatsuListaProfesion.jsp</from-view-id>
	    	<navigation-case>
	    		<from-action>#{profesionController.cargarObjeto}</from-action>
	    		<from-outcome>success</from-outcome>
	    		<to-view-id>/shiatsuActProfesion.jsp</to-view-id>
	    	</navigation-case>
	    	<navigation-case>
	    		<from-action>#{profesionController.irAgregar}</from-action>
	    		<from-outcome>success</from-outcome>
	    		<to-view-id>/shiatsuActProfesion.jsp</to-view-id>
	    	</navigation-case>
	    </navigation-rule>
		<navigation-rule>
	    	<from-view-id>/shiatsuActProfesion.jsp</from-view-id>
	    	<navigation-case>
	    		<from-action>#{profesionController.regresar}</from-action>
	    		<from-outcome>success</from-outcome>
	    		<to-view-id>/shiatsuListaProfesion.jsp</to-view-id>
	    	</navigation-case>
	    	<navigation-case>
	    		<from-action>#{profesionController.eliminar}</from-action>
	    		<from-outcome>success</from-outcome>
	    		<to-view-id>/shiatsuListaProfesion.jsp</to-view-id>
	    	</navigation-case>
	    </navigation-rule>
	    
	    
	    
			<!-- ================ Navigation TipoAlopeciaController ================ -->
	 <navigation-rule>
	    	<from-view-id>/shiatsuListaTipoAlopecia.jsp</from-view-id>
	    	<navigation-case>
	    		<from-action>#{tipoAlopeciaController.cargarObjeto}</from-action>
	    		<from-outcome>success</from-outcome>
	    		<to-view-id>/shiatsuActTipoAlopecia.jsp</to-view-id>
	    	</navigation-case>
	    	<navigation-case>
	    		<from-action>#{tipoAlopeciaController.irAgregar}</from-action>
	    		<from-outcome>success</from-outcome>
	    		<to-view-id>/shiatsuActTipoAlopecia.jsp</to-view-id>
	    	</navigation-case>
	    </navigation-rule>
		<navigation-rule>
	    	<from-view-id>/shiatsuActTipoAlopecia.jsp</from-view-id>
	    	<navigation-case>
	    		<from-action>#{tipoAlopeciaController.regresar}</from-action>
	    		<from-outcome>success</from-outcome>
	    		<to-view-id>/shiatsuListaTipoAlopecia.jsp</to-view-id>
	    	</navigation-case>
	    	<navigation-case>
	    		<from-action>#{tipoAlopeciaController.eliminar}</from-action>
	    		<from-outcome>success</from-outcome>
	    		<to-view-id>/shiatsuListaTipoAlopecia.jsp</to-view-id>
	    	</navigation-case>
	    </navigation-rule>
		
		
</faces-config>
